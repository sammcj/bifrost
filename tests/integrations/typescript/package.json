{
  "name": "bifrost-integration-tests-typescript",
  "version": "0.1.0",
  "description": "TypeScript integration tests for Bifrost AI gateway",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
